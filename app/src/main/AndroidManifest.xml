<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.go_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoApp">
        <activity
            android:name=".Go_TicketSuccess"
            android:exported="true" />
        <activity
            android:name=".Go_Route_Details"
            android:exported="true" />
        <activity
            android:name=".Go_BusSeatSelect"
            android:exported="true">
        </activity>
        <activity
            android:name=".Go_BusSearch"
            android:exported="true">

        </activity>
        <activity
            android:name=".Go_BusList"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Go_BusSearch" />
        </activity>
        <activity
            android:name=".Go_PaymentMethod"
            android:exported="true" />
        <activity
            android:name=".Go_SeatBooking"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Go_BusList" />
        </activity>
        <activity
            android:name=".Go_User"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Go_Amount" />
        </activity>
        <activity
            android:name=".Go_Amount"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Go_SeatBooking" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" >

            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>